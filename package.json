{
  "name": "discord-worker",
  "version": "1.0.0",
  "description": "Fun experiment with Cloudflare workers and a discord bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "wrangler dev --env dev --local --port 8787",
    "fix": "eslint --fix . --ext .ts",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "test:e2e": "vitest --config ./test/e2e/vitest.e2e.config.ts",
    "deploy_commands": "tsc --project ./scripts/tsconfig.json && node ./bin/scripts/deploy_commands.js",
    "publish:test": "wrangler deploy --env test",
    "publish:prod": "wrangler deploy --env prod"
  },
  "dependencies": {
    "@noble/ed25519": "^2.3.0",
    "discord-api-types": "^0.37.35",
    "itty-router": "^2.6.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250820.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.25.0",
    "typescript": "^4.6.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.31.0"
  }
}
