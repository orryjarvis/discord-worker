{
  "name": "discord-worker",
  "version": "1.0.0",
  "description": "Fun experiment with Cloudflare workers and a discord bot",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "wrangler dev --env dev",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix . --ext .ts",
    "lint": "eslint . --ext .ts",
    "test": "c8 ts-mocha -n loader=ts-node/esm test/**/*ts",
    "deploy_commands": "tsc --project ./scripts/tsconfig.json && node ./bin/scripts/deploy_commands.js",
    "publish:test": "wrangler publish --env test",
    "publish:prod": "wrangler publish --env prod"
  },
  "dependencies": {
    "@noble/ed25519": "^1.6.0",
    "discord-api-types": "^0.37.35",
    "itty-router": "^2.6.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230228.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.54.0",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "eslint": "^8.25.0",
    "ngrok": "^4.3.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.4",
    "wrangler": "^2.12.0"
  }
}